{
	"semi": true,
	"singleQuote": true,
	"tabWidth": 4,
	"useTabs": false,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"arrowParens": "always",
	"vueIndentScriptAndStyle": true,
	"endOfLine": "lf",
	"printWidth": 120,
	"htmlWhitespaceSensitivity": "ignore",
	"plugins": ["prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.scss",
			"options": {
				"singleQuote": false,
				"parser": "scss"
			}
		},
		{
			"files": "*.vue",
			"options": {
				"parser": "vue"
			}
		},
		{
			"files": ["*.ts", "*.js"],
			"options": {
				"parser": "typescript"
			}
		}
	]
}
